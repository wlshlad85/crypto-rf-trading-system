{
  "data": {
    "symbols": [
      "bitcoin",
      "ethereum",
      "tether",
      "solana",
      "binancecoin",
      "usd-coin",
      "ripple",
      "dogecoin",
      "cardano"
    ],
    "coingecko_api_key": "",
    "coinmarketcap_api_key": "",
    "vs_currency": "usd",
    "days": 730,
    "interval": "hourly",
    "cache_dir": "data/cached_data",
    "cache_expiry_hours": 1,
    "requests_per_minute": 25
  },
  "features": {
    "rsi_period": 14,
    "macd_fast": 12,
    "macd_slow": 26,
    "macd_signal": 9,
    "bb_period": 20,
    "bb_std": 2.0,
    "atr_period": 14,
    "ema_periods": [
      12,
      26,
      50
    ],
    "sma_periods": [
      20,
      50,
      200
    ],
    "volume_sma_period": 20,
    "obv_period": 10,
    "return_periods": [
      1,
      6,
      12,
      24
    ],
    "volatility_periods": [
      24,
      168,
      720
    ],
    "correlation_period": 168,
    "max_features": 50,
    "feature_importance_threshold": 0.01
  },
  "model": {
    "n_estimators": 200,
    "max_depth": 10,
    "min_samples_split": 10,
    "min_samples_leaf": 5,
    "max_features": "sqrt",
    "bootstrap": true,
    "n_jobs": -1,
    "random_state": 42,
    "use_grid_search": true,
    "cv_folds": 5,
    "scoring": "neg_mean_squared_error",
    "test_size": 0.2,
    "validation_size": 0.2,
    "walk_forward_splits": 10,
    "target_horizon": 6,
    "target_type": "returns"
  },
  "strategy": {
    "long_threshold": 0.6,
    "short_threshold": 0.4,
    "max_position_size": 0.15,
    "min_position_size": 0.05,
    "position_sizing_method": "equal_weight",
    "rebalancing_frequency": "monthly",
    "rebalancing_threshold": 0.05,
    "max_portfolio_leverage": 2.0,
    "stop_loss_pct": 0.1,
    "take_profit_pct": 0.2,
    "max_drawdown_pct": 0.2,
    "transaction_cost_pct": 0.001,
    "slippage_pct": 0.0005
  },
  "backtest": {
    "start_date": "2022-01-01",
    "end_date": "2024-12-31",
    "initial_capital": 100000.0,
    "benchmark_symbol": "bitcoin",
    "risk_free_rate": 0.02,
    "save_results": true,
    "results_dir": "results",
    "plot_results": true
  },
  "system": {
    "log_level": "INFO",
    "log_file": "crypto_rf_trading.log",
    "n_jobs": -1,
    "memory_limit_gb": 8.0,
    "data_dir": "data",
    "models_dir": "models",
    "results_dir": "results",
    "enable_alerts": false,
    "alert_email": "",
    "random_seed": 42
  }
}