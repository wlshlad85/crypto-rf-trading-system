{
  "data": {
    "symbols": ["bitcoin"],
    "coingecko_api_key": "",
    "coinmarketcap_api_key": "",
    "vs_currency": "usd",
    "days": 60,
    "interval": "5m",
    "cache_dir": "data/cached_data",
    "cache_expiry_hours": 1,
    "requests_per_minute": 25,
    "data_source": "multi_timeframe",
    "use_ultra_features": true,
    "use_ultra_targets": true
  },
  "features": {
    "rsi_period": 14,
    "macd_fast": 12,
    "macd_slow": 26,
    "macd_signal": 9,
    "bb_period": 20,
    "bb_std": 2.0,
    "atr_period": 14,
    "ema_periods": [5, 12, 26, 50],
    "sma_periods": [10, 20, 50, 100],
    "volume_sma_period": 20,
    "obv_period": 10,
    "return_periods": [1, 3, 6, 12, 24, 48],
    "volatility_periods": [24, 168, 720],
    "correlation_period": 288,
    "max_features": 200,
    "feature_importance_threshold": 0.005,
    "use_ultra_engineering": true,
    "feature_selection_method": "recursive",
    "interaction_features": true,
    "polynomial_features": false
  },
  "model": {
    "model_type": "ensemble",
    "ensemble_models": ["random_forest", "xgboost", "lightgbm"],
    "ensemble_method": "weighted_voting",
    "n_estimators": 500,
    "max_depth": 15,
    "min_samples_split": 5,
    "min_samples_leaf": 2,
    "max_features": "sqrt",
    "bootstrap": true,
    "n_jobs": -1,
    "random_state": 42,
    "use_optuna_optimization": false,
    "optuna_trials": 200,
    "optuna_timeout": 7200,
    "multi_objective": true,
    "objectives": ["accuracy", "sharpe_ratio", "max_drawdown"],
    "cv_folds": 8,
    "scoring": "neg_mean_squared_error",
    "test_size": 0.15,
    "validation_size": 0.15,
    "walk_forward_splits": 12,
    "purged_cv": true,
    "embargo_periods": 24,
    "target_horizon": 6,
    "target_type": "meta_sharpe",
    "target_preprocessing": "quantile"
  },
  "strategy": {
    "long_threshold": 0.58,
    "short_threshold": 0.42,
    "max_position_size": 0.25,
    "min_position_size": 0.03,
    "position_sizing_method": "volatility_adjusted",
    "rebalancing_frequency": "hourly",
    "rebalancing_threshold": 0.02,
    "max_portfolio_leverage": 1.8,
    "stop_loss_pct": 0.03,
    "take_profit_pct": 0.08,
    "max_drawdown_pct": 0.12,
    "transaction_cost_pct": 0.0008,
    "slippage_pct": 0.0003,
    "risk_adjusted_sizing": true,
    "dynamic_thresholds": true,
    "regime_aware_strategy": true
  },
  "backtest": {
    "start_date": "2024-10-01",
    "end_date": "2024-12-31",
    "initial_capital": 10000.0,
    "benchmark_symbol": "bitcoin",
    "risk_free_rate": 0.05,
    "save_results": true,
    "results_dir": "results/ultra_optimized",
    "plot_results": true,
    "detailed_analysis": true,
    "regime_analysis": true,
    "risk_metrics": true
  },
  "ultra_optimization": {
    "multi_timeframe_analysis": true,
    "primary_timeframe": "5m",
    "secondary_timeframes": ["1h", "15m"],
    "advanced_feature_engineering": true,
    "risk_adjusted_targets": true,
    "regime_aware_modeling": true,
    "ensemble_stacking": true,
    "hyperparameter_search_space": {
      "n_estimators": [300, 500, 800, 1000],
      "max_depth": [10, 15, 20, 25],
      "learning_rate": [0.01, 0.05, 0.1, 0.2],
      "subsample": [0.7, 0.8, 0.9, 1.0],
      "colsample_bytree": [0.7, 0.8, 0.9, 1.0],
      "min_child_weight": [1, 3, 5, 7],
      "gamma": [0, 0.1, 0.2, 0.3],
      "reg_alpha": [0, 0.1, 0.5, 1.0],
      "reg_lambda": [0, 0.1, 0.5, 1.0]
    },
    "feature_engineering_config": {
      "fractal_features": true,
      "microstructure_features": true,
      "regime_features": true,
      "volatility_clustering": true,
      "order_flow_features": true,
      "time_decay_features": true,
      "cross_timeframe_features": true
    },
    "target_engineering_config": {
      "sharpe_targets": true,
      "sortino_targets": true,
      "regime_conditional_targets": true,
      "tail_risk_targets": true,
      "momentum_targets": true,
      "cross_sectional_targets": false,
      "meta_targets": true
    }
  },
  "system": {
    "log_level": "INFO",
    "log_file": "logs/ultra_btc_trading.log",
    "n_jobs": -1,
    "memory_limit_gb": 6.0,
    "data_dir": "data",
    "models_dir": "models/ultra_optimized",
    "results_dir": "results/ultra_optimized",
    "enable_alerts": false,
    "alert_email": "",
    "random_seed": 42,
    "enable_profiling": true,
    "save_intermediate_results": true,
    "checkpoint_frequency": 50
  }
}